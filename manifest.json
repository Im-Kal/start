{
    "manifest_version": 2,

    "name": "New Tab",
    "description": "This extension provides a custom new tab page.",
    "version": "1.0",

    "background": {
        "persistant": false
    },

    "permissions": [
        "geolocation"
    ],

    "content_security_policy": "script-src 'self' https://query.yahooapis.com/v1/public/yql; object-src 'self'",

    "chrome_url_overrides": {
        "newtab": "index.html"
    }
}
